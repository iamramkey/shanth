DROP TABLE toolbar_item CASCADE CONSTRAINTS;

CREATE TABLE toolbar_item (
tbi_id INTEGER NOT NULL PRIMARY KEY ,
tbr_id INTEGER NOT NULL ,
tbi_order_no INTEGER NOT NULL ,
tbi_name  VARCHAR2(255) NOT NULL,
tbi_url  VARCHAR2(255) NOT NULL,
tbi_icon  VARCHAR2(255) NOT NULL,
tbi_bk  VARCHAR2(255) NOT NULL,
tbi_created_usr_id INTEGER NOT NULL ,
tbi_modified_usr_id INTEGER ,
tbi_created_dttm TIMESTAMP NOT NULL,
tbi_modified_dttm TIMESTAMP,
UNIQUE (tbi_name),
UNIQUE (tbr_id,tbi_order_no),
UNIQUE (tbi_bk)
);

